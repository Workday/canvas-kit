  "theme": {
    "button": {
      "primary": {
        "background": {
          "$type": "color",
          "$value": "{accent.default}"
        },
        "text": {
          "$type": "color",
          "$value": "{accent.onAccent}"
        }
      },
      "borderRadius": {
        "$type": "borderRadius",
        "$value": "{borderRadius.lg}"
      },
      "borderWidth": {
        "$type": "borderWidth",
        "$value": "{dimension.sm}"
      }
    },
    "card": {
      "borderRadius": {
        "$type": "borderRadius",
        "$value": "{borderRadius.lg}"
      },
      "background": {
        "$type": "color",
        "$value": "{bg.default}"
      },
      "padding": {
        "$type": "dimension",
        "$value": "{dimension.md}"
      }
    },
    "boxShadow": {
      "default": {
        "$type": "boxShadow",
        "$value": [
          {
            "x": 5,
            "y": 5,
            "spread": 3,
            "color": "rgba({shadows.default}, 0.15)",
            "blur": 5,
            "$type": "dropShadow"
          },
          {
            "x": 4,
            "y": 4,
            "spread": 6,
            "color": "#00000033",
            "blur": 5,
            "$type": "innerShadow"
          }
        ]
      }
    },
    "typography": {
      "H1": {
        "Bold": {
          "$type": "typography",
          "$value": {
            "fontFamily": "{fontFamilies.heading}",
            "fontWeight": "{fontWeights.headingBold}",
            "lineHeight": "{lineHeights.heading}",
            "fontSize": "{fontSizes.h1}",
            "paragraphSpacing": "{paragraphSpacing.h1}",
            "letterSpacing": "{letterSpacing.decreased}"
