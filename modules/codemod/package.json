{
  "name": "@workday/canvas-kit-codemod",
  "author": "Workday, Inc. (https://www.workday.com)",
  "license": "Apache-2.0",
  "version": "0.0.0",
  "description": "A collection of codemods for use on Workday Canvas Kit packages.",
  "main": "dist/commonjs/index.js",
  "module": "dist/es6/index.js",
  "sideEffects": false,
  "bin": {
    "canvas-kit-codemod": "dist/commonjs/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Workday/canvas-kit.git"
  },
  "files": [
    "dist/",
    "index.ts"
  ],
  "keywords": [
    "workday",
    "canvas",
    "kit",
    "codemod"
  ],
  "bugs": {
    "url": "https://github.com/Workday/canvas-kit/issues"
  },
  "homepage": "https://github.com/Workday/canvas-kit#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "jscodeshift": "^0.11.0",
    "yargs": "^16.2.0"
  },
  "scripts": {
    "clean": "rimraf dist && rimraf .build-info && mkdirp dist",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:es6": "tsc -p tsconfig.es6.json",
    "build:rebuild": "npm-run-all clean build",
    "build": "npm-run-all --parallel build:cjs build:es6",
    "test": "TZ=UTC jest -c ../../jest.config.js"
  }
}
